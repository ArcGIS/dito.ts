{
  "name": "DiTO",
  "version": "0.1.0",
  "description": "Fast Computation of Tight-Fitting Oriented Bounding Boxes",
  "main": "src/dito.js",
  "jsnext:main": "esm/dito.js",
  "module": "esm/dito.js",
  "types": "src/dito.d.ts",
  "scripts": {
    "prepublish": "node_modules/.bin/gulp clean && npm run tsc && npm run rollup && npm run rollup:min",
    "tsc": "node_modules/.bin/tsc -p .",
    "karma": "node_modules/.bin/karma start karma.conf.js",
    "test": "npm run karma",
    "rollup": "node_modules/.bin/rollup -c",
    "rollup:min": "node_modules/.bin/rollup -c --environment BUNDLE_MIN",
    "cover": "cat ./coverage/*/lcovonly | ./node_modules/.bin/coveralls"
  },
  "author": "Stefan Eilemann",
  "repository": {
    "type": "git",
    "url": "https://github.com/eile/dito.ts.git"
  },
  "keywords": [
    "dito",
    "obb",
    "geometry",
    "orientedBoundingBox",
    "library"
  ],
  "license": "BSD",
  "devDependencies": {
    "core-js": "^2.4.1",
    "coveralls": "^2.11.15",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-typescript": "^3.0.9",
    "reflect-metadata": "^0.1.3",
    "rollup": "^0.41.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rxjs": "^5.0.3",
    "typescript": "^2.0.8",
    "zone.js": "^0.8.4"
  },
  "dependencies": {
    "@types/gl-matrix": "^2.4.0",
    "gl-matrix": "2.4.0"
  }
}
